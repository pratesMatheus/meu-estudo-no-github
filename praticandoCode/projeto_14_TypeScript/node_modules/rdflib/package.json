{
  "name": "rdflib",
  "description": "an RDF library for node.js. Suitable for client and server side.",
  "version": "0.19.1",
  "engines": {
    "node": ">=6.0"
  },
  "private": false,
  "author": {
    "name": "Tim BL",
    "email": "timbl@w3.org"
  },
  "contributors": [
    {
      "name": "Ruben Verborgh",
      "url": "https://github.com/RubenVerborgh"
    },
    {
      "name": "Dmitri Zagidulin",
      "url": "https://github.com/dmitrizagidulin/"
    },
    {
      "name": "Daniel Friedman",
      "url": "https://github.com/dan-f/"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/linkeddata/rdflib.js.git"
  },
  "homepage": "http://github.com/linkeddata/rdflib.js",
  "bugs": "http://github.com/linkeddata/rdflib.js/issues",
  "dependencies": {
    "async": "^0.9.x",
    "jsonld": "^0.4.5",
    "n3": "^0.4.1",
    "xmldom": "^0.1.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "chai": "^4.1.0",
    "diff": "^3.2.0",
    "dirty-chai": "^2.0.0",
    "fs-grep": "^0.0.5",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nock": "^9.0.13",
    "node-fetch": "^1.7.0",
    "node-mkdirp": "0.0.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0",
    "webpack": "^4.20.1",
    "webpack-cli": "^3.1.1",
    "webpack-sources": "^1.0.1",
    "wrapper-webpack-plugin": "^2.0.0"
  },
  "scripts": {
    "build": "babel src -d lib",
    "build:browser": "webpack --progress",
    "doc": "rm -r doc && jsdoc -d doc README.md src/*.js",
    "prepare": "npm run build && npm run build:browser",
    "test": "npm run test:unit",
    "test:unit": "mocha  --compilers js:@babel/register tests/unit/**-test.js",
    "test:unit:dev": "mocha --watch  --compilers js:@babel/register tests/unit/**-test.js"
  },
  "files": [
    "src",
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "keywords": [
    "linkeddata",
    "linked data",
    "rdf",
    "rdfa",
    "turtle",
    "semantic",
    "web",
    "read-write web"
  ],
  "standard": {
    "globals": [
      "ActiveXObject",
      "Components",
      "DOMParser",
      "netscape",
      "$rdf",
      "Services",
      "tabulator",
      "WebSocket"
    ]
  }
}
